<table class="table table-bordered">
  <thead class="thead-light">
    <tr>
      <th>ジャンル検索</th>
    </tr>
  </thead>
  <% genres.each do |genre| %>
    <tbody>
      <tr>
        <td>
          <%= link_to genre.name, items_path(genre_id: genre.id) %>
        </td>
      </tr>
    </tbody>
  <% end %>
</table>
